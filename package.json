{
  "name": "frontendperformer",
  "version": "12.21.13",
  "description": "Vue - Vuetify - TS - Webpack5",
  "author": "Qscr",
  "main": "main.ts",
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "scripts": {
    "build": "webpack --config config/webpack.prod.js",
    "debug": "webpack --verbose --inspect --config config/webpack.dev.js",
    "depcheck": "npx depcheck",
    "npm-check": "npm-check",
    "npm-check-install": "npm install -g npm-check",
    "npm-check:u": "npm-check -u",
    "start": "node index.js",
    "watch": "webpack --watch --config config/webpack.dev.js"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.178",
    "@types/webpack-dev-middleware": "^5.0.2",
    "@types/webpack-env": "^1.16.3",
    "@types/webpack-hot-middleware": "^2.25.5",
    "@vue/cli-plugin-pwa": "^4.5.15",
    "@vuex-orm/core": "^0.35.1",
    "@vuex-orm/plugin-axios": "^0.9.3",
    "axios": "^0.21.4",
    "connect-history-api-fallback": "^1.6.0",
    "dart-sass": "^1.25.0",
    "debounce": "^1.2.0",
    "express": "^4.17.1",
    "hyphen": "^1.6.4",
    "jsdom-global": "^3.0.2",
    "register-service-worker": "^1.7.2",
    "vue": "^2.6.14",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^9.1.0",
    "vue-router": "^3.5.3",
    "vue-style-loader": "^4.1.3",
    "vuetify": "^2.6.1",
    "vuetify-loader": "^1.7.3",
    "vuex": "^3.6.2",
    "vuex-map-fields": "^1.4.1",
    "vuex-module-decorators": "^1.0.1",
    "vuex-orm-decorators": "^1.2.2",
    "vuex-persistedstate": "^4.1.0",
    "wolfy87-eventemitter": "^5.2.9",
    "workbox-webpack-plugin": "^6.4.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "alias": {
      "vue": "./node_modules/vue/dist/vue.common.js"
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "keywords": [
    "vue",
    "vuetify",
    "webpack5"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@mdi/font": "^6.5.95",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/babel-preset-app": "^4.5.15",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.4.1",
    "eslint-plugin-vue": "^8.2.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "material-design-icons-iconfont": "^6.1.1",
    "mini-css-extract-plugin": "^2.4.5",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.3",
    "vue-eslint-parser": "^8.0.1",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.2.2",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
